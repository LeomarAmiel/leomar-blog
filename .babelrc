{
  "presets": ["next/babel", "@zeit/next-typescript/babel"],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "extensions": [".ts", ".tsx"],
        "alias": {
          "@assets": "./assets",
          "@pages": "./pages",
          "@components": "./components",
          "@store": "./store",
          "@containers": "./components/containers",
          "@utils": "./utils"
        }
      }
    ]
  ]
}
